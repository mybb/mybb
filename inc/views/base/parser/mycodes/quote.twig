<blockquote class="post__quote">
    <cite>
        {% if delete_quote == false %}<span>{{ quote.date|raw }}</span>{% endif %}
        {{ quote.username }} {{ lang.wrote }}{% if quote.linkback %} <a href="{{ quote.url }}" class="quick_jump"></a>{% endif %}
    </cite>
    {{ quote.message|raw }}
</blockquote>
